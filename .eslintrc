{
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"node": true
	},
	"extends": "airbnb",
	"rules": {
		"indent": ["error", "tab", { "SwitchCase": 1 }],
		"jsx-a11y/anchor-is-valid": "off",
		"linebreak-style": "off",
		"no-tabs": "off",
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
		"react/jsx-indent-props": "tab",
		"react/jsx-indent": "tab",
	}
}
