{
  "name": "note-me",
  "version": "1.0.2",
  "private": true,
  "dependencies": {
    "firebase": "^5.8.4",
    "framework7": "^4.0.5",
    "framework7-icons": "^2.2.0",
    "framework7-react": "^4.0.5",
    "material-design-icons": "^3.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-markdown": "^4.0.6",
    "react-scripts": "^2.1.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
