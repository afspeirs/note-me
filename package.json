    "cpy-cli": "^2.0.0",
    "eslint-config-airbnb": "^17.1.0"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
