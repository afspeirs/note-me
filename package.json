{
  "name": "note-me",
  "private": true,
  "version": "2.2.0-develop.4",
  "type": "module",
  "scripts": {
    "start": "vite dev",
    "dev": "vite dev",
    "prebuild": "npm run semantic-release:dry",
    "build": "vite build",
    "preview": "vite preview",
    "pwa": "npm run build && npm run preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "type": "npm run check",
    "semantic-release:dry": "semantic-release --dry-run --no-ci",
    "outdated": "npx npm-check-updates",
    "outdated:minor": "npx npm-check-updates -t minor",
    "update": "npm run outdated -- -u",
    "update:minor": "npm run outdated:minor -- -u"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/js": "^9.33.0",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@stylistic/eslint-plugin": "^5.2.3",
    "@sveltejs/adapter-netlify": "^5.2.0",
    "@sveltejs/kit": "^2.31.0",
    "@sveltejs/vite-plugin-svelte": "^6.1.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.12",
    "@types/node": "^24.3.0",
    "@types/wicg-file-system-access": "^2023.10.6",
    "@vite-pwa/sveltekit": "^1.0.0",
    "bits-ui": "^2.9.3",
    "eslint": "^9.33.0",
    "eslint-plugin-svelte": "^3.11.0",
    "globals": "^16.3.0",
    "idb": "^8.0.3",
    "lucide-svelte": "^0.539.0",
    "marked": "^16.1.2",
    "melt": "^0.38.0",
    "semantic-release": "^24.2.7",
    "svelte": "^5.38.1",
    "svelte-check": "^4.3.1",
    "tailwindcss": "^4.1.12",
    "tailwindcss-safe-area": "^1.1.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
