{
  "name": "note-me",
  "private": true,
  "version": "2.1.0",
  "type": "module",
  "scripts": {
    "start": "vite dev",
    "dev": "vite dev",
    "prebuild": "npm run semantic-release:dry",
    "build": "vite build",
    "preview": "vite preview",
    "pwa": "npm run build && npm run preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "type": "npm run check",
    "semantic-release:dry": "semantic-release --dry-run --no-ci",
    "outdated": "npx npm-check-updates",
    "outdated:minor": "npx npm-check-updates -t minor",
    "update": "npm run outdated -- -u",
    "update:minor": "npm run outdated:minor -- -u"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@melt-ui/pp": "^0.3.2",
    "@melt-ui/svelte": "^0.86.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@stylistic/eslint-plugin": "^2.13.0",
    "@sveltejs/adapter-netlify": "^4.4.1",
    "@sveltejs/kit": "^2.16.1",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.10.10",
    "@types/wicg-file-system-access": "^2023.10.5",
    "@vite-pwa/sveltekit": "^0.6.7",
    "autoprefixer": "^10.4.20",
    "bits-ui": "^0.22.0",
    "eslint": "^9.18.0",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.14.0",
    "idb": "^8.0.1",
    "lucide-svelte": "^0.474.0",
    "marked": "^15.0.6",
    "semantic-release": "^24.2.1",
    "svelte": "^5.19.2",
    "svelte-check": "^4.1.4",
    "tailwindcss": "^3.4.17",
    "tailwindcss-safe-area": "^0.6.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0",
    "vite": "^6.0.11"
  }
}
